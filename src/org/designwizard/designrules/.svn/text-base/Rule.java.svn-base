package org.designwizard.designrules;

import org.designwizard.exception.InexistentEntityException;

public interface Rule {
	
	/**
	 * This method checks whether the code are in conformance
	 * with the rule or not.
	 * @return true if the code is in conformance with the rule; false otherwise.
	 * @throws InexistentEntityException
	 */
	public boolean checkRule() throws InexistentEntityException;
	
	public String getReport();

}