
public class Inteiro {

	/**
	 * Metodo que retorna se uma string e um inteiro
	 * @param entrada
	 * @return booleana
	 */
	public static boolean ehInteiro(String entrada){
		try{
			Integer.parseInt(entrada);
		}catch(NumberFormatException retorno1){
			return false;
		}
		
		return true;	
	}
	
	/**
	 * Metodo que retorna se um char e um inteiro
	 * @param entrada
	 * @return booleana
	 */
	public static boolean ehInteiro(char entrada){
		try{
			Integer.parseInt(entrada + "");
		}catch(NumberFormatException retorno1){
			return false;
		}
		
		return true;	
	}
}
